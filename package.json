{
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@rails/webpacker": "^4.0.7",
    "awesomplete": "^1.1.2",
    "coffeescript": "1.12.7",
    "jquery": "~> 3.4.0",
    "jquery-ujs": "^1.2.2",
    "lodash": ">=4.17.13"
  },
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:jsx-a11y/strict",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "flowtype",
      "import"
    ],
    "env": {
      "browser": "true"
    },
    "rules": {
      "import/no-cycle": "off",
      "object-curly-newline": "off",
      "react/jsx-filename-extension": "off",
      "semi": [
        "error",
        "never"
      ]
    }
  }
}
